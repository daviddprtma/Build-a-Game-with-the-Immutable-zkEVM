{
  "name": "@imtbl/react-analytics",
  "version": "0.2.1-alpha",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "sideEffects": false,
  "scripts": {
    "lint": "npm run typecheck && npm run lint:eslint",
    "lint:eslint": "TIMING=1 eslint \"**/*.ts*\"",
    "typecheck": "tsc --noEmit",
    "prebuild": "rm -rf ./dist",
    "build": "npm run build:ts",
    "pack": "npm pack",
    "build:ts": "tsup src/index.ts --dts",
    "test": "npm run test:jest:run",
    "test:jest:run": "NODE_ENV=test jest",
    "test:jest:ci": "NODE_ENV=test jest --coverage",
    "publish-package": "if can-npm-publish; then npm publish --access public; else echo 'Skipping package npm publish'; fi"
  },
  "dependencies": {
    "@segment/analytics-next": "^1.53.2",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@jest/globals": "^29.5.0",
    "@types/node": "^18.15.11",
    "babel-jest": "^29.5.0",
    "can-npm-publish": "^1.3.6",
    "eslint": "^8.43.0",
    "eslint-config-custom": "*",
    "jest": "^29.5.0",
    "jest-config": "*",
    "nyc-config": "*",
    "ts-config": "*",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5"
  }
}