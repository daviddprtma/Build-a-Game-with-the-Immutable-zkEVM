{
  "name": "@biom3/react",
  "version": "0.19.15",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "sideEffects": false,
  "scripts": {
    "lint": "npm run typecheck && npm run lint:eslint",
    "lint:eslint": "TIMING=1 eslint \"**/*.ts*\"",
    "lint:eslint:fix": "TIMING=1 eslint \"**/*.ts*\" --fix",
    "dev": "",
    "test": "npm run test:cy:run; npm run test:jest:run",
    "test:cy:ci": "NODE_ENV=test cypress run --component --env coverage=true --record --parallel",
    "test:cy:open": "NODE_ENV=test cypress open --component",
    "test:cy:run": "NODE_ENV=test cypress run --component",
    "test:cy:run:localcov": "NODE_ENV=test cypress run --component --env coverage=true",
    "test:jest:run": "NODE_ENV=test jest",
    "test:jest:ci": "NODE_ENV=test jest --coverage",
    "typecheck": "tsc --noEmit",
    "coverage": "npx nyc report --check-coverage",
    "prebuild": "rm -rf ./dist",
    "build": "tsup src/index.ts --dts",
    "pack": "npm pack",
    "publish-package": "if can-npm-publish; then npm publish --access public; else echo 'Skipping package npm publish'; fi"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@cypress/code-coverage": "^3.11.0",
    "@emotion/babel-plugin": "^11.10.6",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@emotion/cache": "^11.11.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jest/globals": "^29.5.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.throttle": "^4.1.7",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.9",
    "@types/react-is": "^17.0.3",
    "babel-jest": "^29.5.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-rewire-exports": "^2.3.0",
    "can-npm-publish": "^1.3.6",
    "cypress": "^13.5.1",
    "cypress-real-events": "^1.10.3",
    "eslint-config-custom": "*",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "jest": "^29.5.0",
    "jest-config": "*",
    "jest-environment-jsdom": "^29.7.0",
    "next": "~13.4.19",
    "next-router-mock": "^0.9.10",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "nyc-config": "*",
    "react-router-dom": "^6.14.2",
    "ts-config": "*",
    "ts-jest": "^29.1.0",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5",
    "whatwg-fetch": "^3.6.19"
  },
  "dependencies": {
    "@biom3/design-tokens": "*",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@rive-app/react-canvas": "^4.5.0",
    "buffer": "^6.0.3",
    "csstype": "^3.1.2",
    "framer-motion": "^10.12.12",
    "localstorage-slim": "^2.7.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "ts-deepmerge": "^6.2.0"
  }
}