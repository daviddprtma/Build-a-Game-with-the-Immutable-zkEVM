{
  "name": "@biom3/design-tokens",
  "version": "0.2.9",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "directories": {
    "css": "css"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "scripts": {
    "lint": "npm run typecheck && npm run lint:eslint",
    "lint:eslint": "TIMING=1 eslint \"**/*.ts*\"",
    "typecheck": "tsc --noEmit",
    "prebuild": "rm -rf ./dist",
    "build": "npm run build:tokens; npm run build:ts",
    "pack": "npm pack",
    "prebuild:tokens": "rm  -rf ./css; mkdir css;",
    "build:tokens": "ts-node ./src/packageScripts/dumpTokens.ts",
    "build:ts": "tsup src/index.ts --dts",
    "publish-package": "if can-npm-publish; then npm publish --access public; else echo 'Skipping package npm publish'; fi"
  },
  "dependencies": {
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "can-npm-publish": "^1.3.6",
    "eslint": "^8.43.0",
    "eslint-config-custom": "*",
    "nodemon": "^2.0.20",
    "prepend-file": "^2.0.1",
    "ts-config": "*",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^4.9.5"
  }
}